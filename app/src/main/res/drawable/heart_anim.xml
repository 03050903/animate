<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
        android:width="120dp"
        android:height="120dp"
        android:viewportHeight="56"
        android:viewportWidth="56">

    <path
        android:fillColor="@color/fab_heart_empty"
        android:pathData="@string/heart_fill_empty_path" />

    <group android:name="filled">

        <clip-path
            android:name="clip"
            android:pathData="@string/heart_fill_clip_hidden" />

        <path
            android:fillColor="@color/fab_heart_full"
            android:pathData="@string/heart_fill_full_path" />

    </group>

</vector>